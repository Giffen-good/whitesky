
 .collection .tile {
   	height:45vw;
   
 }
 .giant-tile {
     margin-bottom:0 !important;
   }
.giant-tile .x1033.tile {
	height:45vw;
  margin-bottom:0 !important;
}
.image-with-text.featured-item img {
  padding-bottom:0 !important;
}
@media screen and (max-width: 768px) {
 .giant-tile .x1033.tile {
      max-width: 50vw;
      height: 68vw;
	}
}
#full-width-image {
  z-index:1;
}
.sticky-container {
	z-index:2;
}
.inner-fix {
	float:left !important;
}
.sticky-container {
	flex-grow:1;
}
.lat, .lng {
	position:absolute;
  bottom:0;
}
#shopify-section-about p {
