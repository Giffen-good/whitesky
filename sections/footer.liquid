<footer sm="psr pt1-5" class="pt2">

  {% if section.settings.show_payment_icons %}
    {% unless shop.enabled_payment_types == empty %}
      <span class="visually-hidden">{{ 'layout.footer.payment_methods' | t }}</span>
      <ul class="payment-icons">
        {% for type in shop.enabled_payment_types %}
          <li>
            {{ type | payment_type_svg_tag: class: 'icon' }}
          </li>
        {% endfor %}
      </ul>
    {% endunless %}
  {% endif %}



  <div class="x w100">
    <div class="x x1050 w100">



      {%- assign footer_linklist = section.settings.social_linklist -%}
        {% if linklists[footer_linklist].links.size > 0 %}
          <ul sm="mt0 pl1" class="x1050 mt0">
            {% for link in linklists[footer_linklist].links %}
            {% if  link.title == 'Newsletter' %}
              <li id="newsletter-link"><a href="{{ link.url }}">{{ link.title }}</a></li>

            {% else %}
              <li><a href="{{ link.url }}">{{ link.title }}</a></li>

            {% endif %}
            {% endfor %}
          </ul>
        {% endif %}
        {%- assign footer_linklist = section.settings.footer_linklist -%}
        {% if linklists[footer_linklist].links.size > 0 %}
          <ul sm="mt0 pl3" class="x1050 mt0">
            {% for link in linklists[footer_linklist].links %}
              <li><a href="{{ link.url }}">{{ link.title }}</a></li>
            {% endfor %}
          </ul>
        {% endif %}
    </div>
    <div sm="" class="xs1 x1050">
      {% unless template == "page.newsletter" %}
      <form action="https://test.us16.list-manage.com/subscribe/post-json?u=37cf957e30bf22bff508ea6c6&amp;id=aeeb24162f&c=?" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate"  novalidate>
      <label for="mce-EMAIL" id="mceElabel" class="pr1">{{ section.settings.newsletter_text }}</label>
      <div class="" id="newsletterBox">
        <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL">
        <div id="mce-responses" class="clear">
          <div class="response" id="mce-error-response" style="display:none"></div>
          <div class="response" id="mce-success-response" style="display:none"></div>
        </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_37cf957e30bf22bff508ea6c6_aeeb24162f" tabindex="-1" value=""></div>
          <div  class="clear dib"><input type="submit" value="" name="subscribe" id="mc-embedded-subscribe" class="button" style="background-image:url({{'small-arrow.png' | asset_url }})" ></div>
      </div>
      </form>
      <div id="thankYou" class="c9"></div> 
      {% endunless %}
    </div>

  </div>




  <div class='x w100 xjc el-2'>
    <div class="x1050 psr">
      <span class="lat" class=" psa cy"></span>
    </div>
    <div sm="psi tar" class="x1050 psr x xjb">
      <div sm="w100"><span  sm="psi" class="cy psa lng"></span></div>
      <img sm="psa t0 r0 pr1 pt1-5" class="footer-logo" src="{{ 'footer-logo.png' | asset_url }}"/>
    </div>
  </div>
</footer>






<header id="mobile-menu"  class="x psf  xdc xw xjc dn vh100" data-section-id="{{ section.id }}" data-section-type="header">
 


    {% if shop.customer_accounts_enabled %}
      <ul>
        {% if customer %}
          <li>
            {% if customer.first_name != blank %}
              {% capture first_name %}<a href="/account">{{ customer.first_name }}</a>{% endcapture %}
              {{ 'layout.customer.logged_in_as_html' | t: first_name: first_name }}
            {% else %}
              <a href="/account">{{ 'layout.customer.account' | t }}</a>
            {% endif %}
          </li>
          <li>
            {{ 'layout.customer.log_out' | t | customer_logout_link }}
          </li>
        {% else %}
          <li>
            {{ 'layout.customer.log_in' | t | customer_login_link }}
          </li>
          <li>
            {{ 'layout.customer.create_account' | t | customer_register_link }}
          </li>
        {% endif %}
      </ul>
    {% endif %}

    

  <nav sm="xn" class="x1033" role="navigation">
    <ul class="pl0 x xjc ">
      {% for link in linklists[section.settings.mobileMenuTop].links %}
        {% if link.links != blank %}
          <li class="pl1 pr1">
            <a href="{{ link.url }}">
              {{ link.title }}
              {% include 'icon-arrow-down' %}
            </a>
            <ul>
              {% for childlink in link.links %}
                <li class="pl1 pr1">
                  <a href="{{ childlink.url }}">
                    {{ childlink.title }}
                  </a>
                </li>
              {% endfor %}
            </ul>
          </li>
        {% else %}
          <li class="pl1 pr1">
            <a  href="{{ link.url }}">
              {{ link.title }}
            </a>
          </li>
        {% endif %}
      {% endfor %}
      <li> <a class="" href="/cart">Cart({{ cart.item_count }})</a></li>
    </ul>
     <ul class="pl0 pt1 x xjc ">
          {% for link in linklists[section.settings.mobileMenuMid].links %}
            {% if link.links != blank %}
              <li class="pl1 pr1">
                <a href="{{ link.url }}">
                  {{ link.title }}
                  {% include 'icon-arrow-down' %}
                </a>
                <ul>
                  {% for childlink in link.links %}
                    <li class="pl1 pr1">
                      <a href="{{ childlink.url }}">
                        {{ childlink.title }}
                      </a>
                    </li>
                  {% endfor %}
                </ul>
              </li>
            {% else %}
              <li class="pl1 pr1">
                <a  href="{{ link.url }}">
                  {{ link.title }}
                </a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
         <ul class="pl0 pt1 x xjc ">
          {% for link in linklists[section.settings.social_linklist].links %}
            {% if link.links != blank %}
              <li class="pl1 pr1">
                <a href="{{ link.url }}">
                  {{ link.title }}
                  {% include 'icon-arrow-down' %}
                </a>
                <ul>
                  {% for childlink in link.links %}
                    <li class="pl1 pr1">
                      <a href="{{ childlink.url }}">
                        {{ childlink.title }}
                      </a>
                    </li>
                  {% endfor %}
                </ul>
              </li>
            {% else %}
              <li class="pl1 pr1">
                <a  href="{{ link.url }}">
                  {{ link.title }}
                </a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
         <div class="tac x xjc  pt1 xs1 ofc x1033">
    <div class="doubly-wrapper" style="background:url({{ 'simple-down-arrow.png' | asset_url }}) 60% / 17% no-repeat"></div>

  </div>
  </nav>
  <div class='x w100 psf b0 p1 xjc el-2'>
    <div class="x1050 psr">
      <span class="lat" class=" psa cy"></span>
    </div>
    <div sm="psi tar" class="x1050 psr x xjb">
      <div sm="w100"><span  sm="psi" class="cy psa lng"></span></div>
    </div>
  </div>
</header>
<script>
  $(document).ready(function(){
  $('#nav-icon2').click(function(){
    $(this).toggleClass('open');
    $('#mobile-menu').toggleClass('open');
    $('#main-head').toggleClass('open');

    $('#shopify-section-footer').toggleClass('open');
    document.addEventListener('keydown',function(e) {
      if (e.key === "Escape") {
        console.log('e');
           $('#mobile-menu').removeClass('open');
    $('#main-head').removeClass('open');
    $('#nav-icon2').removeClass('open');
      }
    })
  });
});
</script>
{% schema %}
  {
    "name": "Footer",
    "settings": [
      {
        "type": "link_list",
        "id": "footer_linklist",
        "label": "First menu",
        "default": "footer"
      },
      {
        "type": "checkbox",
        "id": "show_payment_icons",
        "label": "Show payment icons",
        "default": false
      },
      {
        "type": "link_list",
        "id": "social_linklist",
        "label": "Social Menu"
      },
      {
        "type": "text",
        "id": "newsletter_text",
        "label": "Newsletter Text",
        "default": "Join our mailing list for 20% off your next order"
      },{
        "type": "link_list",
        "id": "mobileMenuTop",
        "label": "Top Section of Mobile Menu"
      },
      {
        "type": "link_list",
        "id": "mobileMenuMid",
        "label": "Middle Section of Mobile Menu"
      }

    ]
  }

{% endschema %}
